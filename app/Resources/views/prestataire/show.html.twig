{% extends 'layout.html.twig' %}

{% block body %}
    <h1>Prestataire</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ prestataire.id }}</td>
            </tr>
            <tr>
                <th>Compagnie</th>
                <td>{{ prestataire.compagnie }}</td>
            </tr>
            <tr>
                <th>Typeservice</th>
                <td>{{ prestataire.typeService }}</td>
            </tr>
            <tr>
                <th>Adresse</th>
                <td>{{ prestataire.adresse }}</td>
            </tr>
            <tr>
                <th>Phonenumber</th>
                <td>{{ prestataire.phoneNumber }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('prestataire_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('prestataire_edit', { 'id': prestataire.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
